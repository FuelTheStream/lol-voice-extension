# Twitch API credentials
TWITCH_CLIENT_ID=your_client_id
TWITCH_CLIENT_SECRET=your_client_secret

# Broadcaster channel and reward
TWITCH_BROADCASTER_ID=123456789
TWITCH_REWARD_TITLE=Play Champion Voice Line

# OAuth token (broadcaster, with scopes: channel:manage:redemptions channel:read:redemptions)
TWITCH_BROADCASTER_OAUTH_TOKEN=your_broadcaster_token_here

# EventSub webhook secret (for production signature verification)
TWITCH_EVENTSUB_SECRET=some_random_secret

# Cooldown between redemptions, in milliseconds
REDEEM_COOLDOWN_MS=30000
